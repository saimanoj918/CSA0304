#include <stdio.h>
#define SIZE 10

int adj[SIZE][SIZE], visited[SIZE], n;

void dfs(int node) {
    visited[node] = 1;
    printf("%d ", node);
    for(int i = 0; i < n; i++)
        if(adj[node][i] && !visited[i])
            dfs(i);
}

int main() {
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter adjacency matrix:\n");
    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);
    dfs(0);
    return 0;
}
