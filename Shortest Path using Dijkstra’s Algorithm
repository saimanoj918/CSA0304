#include <stdio.h>
#define INF 999
#define SIZE 10

int main() {
    int n, cost[SIZE][SIZE], dist[SIZE], visited[SIZE] = {0}, count, min, next, i, j;
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter cost matrix:\n");
    for(i = 0; i < n; i++)
        for(j = 0; j < n; j++) {
            scanf("%d", &cost[i][j]);
            if(cost[i][j] == 0) cost[i][j] = INF;
        }
    dist[0] = 0;
    for(i = 1; i < n; i++) dist[i] = cost[0][i];
    visited[0] = 1;

    for(count = 1; count < n-1; count++) {
        min = INF;
        for(i = 0; i < n; i++)
            if(!visited[i] && dist[i] < min) {
                min = dist[i];
                next = i;
            }
        visited[next] = 1;
        for(i = 0; i < n; i++)
            if(!visited[i] && min + cost[next][i] < dist[i])
                dist[i] = min + cost[next][i];
    }
    for(i = 0; i < n; i++)
        printf("Distance from 0 to %d = %d\n", i, dist[i]);
}
