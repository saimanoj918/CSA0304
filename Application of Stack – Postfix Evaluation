#include <stdio.h>
#include <ctype.h>
int stack[100], top = -1;
void push(int val) {
    stack[++top] = val;
}
int pop() {
    return stack[top--];
}
int main() {
    char exp[] = "23*5+";
    int i, a, b;
    for (i = 0; exp[i]; i++) {
        if (isdigit(exp[i]))
            push(exp[i] - '0');
        else {
            b = pop();
            a = pop();
            if (exp[i] == '+') push(a + b);
            if (exp[i] == '-') push(a - b);
            if (exp[i] == '*') push(a * b);
            if (exp[i] == '/') push(a / b);
        }
    }
    printf("Result = %d\n", pop());
    return 0;
}
